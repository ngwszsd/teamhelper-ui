{
  "name": "@teamhelper/ui",
  "version": "1.0.15",
  "description": "Component library for Teamhelper",
  "author": "geekbook9533@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.teamhelper.cn/teamhelper-v4/front-end/teamhelper-v4-web"
  },
  "homepage": "https://gitlab.teamhelper.cn/teamhelper-v4/front-end/teamhelper-v4-web",
  "keywords": [
    "react",
    "components",
    "ui",
    "typescript",
    "component-library",
    "tailwind"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "./*": {
        "types": "./dist/*.d.ts",
        "import": "./dist/*.js",
        "require": "./dist/*.cjs"
      }
    }
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./src/*.ts",
      "import": "./src/*.ts",
      "require": "./dist/*.cjs"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.cjs",
    "dist/**/*.d.ts",
    "!dist/**/*.d.ts.map",
    "!dist/**/*.js.map",
    "!dist/build-info.json",
    "!dist/bundle-analysis.json",
    "README.md",
    "README_TREE_SHAKING.md"
  ],
  "scripts": {
    "dev": "rslib build --watch",
    "build": "node scripts/build.js",
    "build:rslib": "rslib build",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "build:types": "tsc --emitDeclarationOnly",
    "pub": "node scripts/publish.js",
    "pub:quick": "npm run build && npm publish --access public",
    "analyze": "node scripts/analyze-bundle.js",
    "analyze:size": "node scripts/analyze-package-size.js",
    "prepublishOnly": "npm run build",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "storybook:dev": "storybook dev -p 6077",
    "storybook:build": "storybook build",
    "docs:local": "pnpm storybook:build && live-server ./storybook-static"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.6.7",
    "@rsbuild/plugin-react": "^1.4.2",
    "@rslib/core": "^0.18.0",
    "@storybook/addon-docs": "^10.0.8",
    "@storybook/addon-onboarding": "^10.0.8",
    "@storybook/react": "^10.0.8",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "autoprefixer": "^10.4.19",
    "live-server": "^1.2.2",
    "postcss": "^8.4.35",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "storybook": "^10.0.8",
    "storybook-addon-rslib": "^2.1.6",
    "storybook-react-rsbuild": "^2.1.6",
    "tailwindcss": "^4.1.17",
    "tslib": "^2.8.1",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@ebay/nice-modal-react": "1.2.13",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.8",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-virtual": "^3.13.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.554.0",
    "nanoid": "^5.1.6",
    "react-day-picker": "^9.11.1",
    "react-hook-form": "^7.66.1",
    "react-i18next": "^16.3.3",
    "react-resizable-panels": "^3.0.6",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "vaul": "^1.1.2"
  }
}
